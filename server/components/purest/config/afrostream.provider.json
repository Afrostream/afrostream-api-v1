{
  "afrostream": {
    "__provider": {
      "oauth2": true,
      "refresh": "https://afrostream-backend.herokuapp.com/auth/oauth2",
      "docs": "https://afrostream-backend.herokuapp.com/doc"
    },
    "http://localhost:9000": {
      "__domain": {
        "auth": {
          "qs": {
            "access_token": "[0]"
          }
        }
      },
      "api/{endpoint}": {
        "__path": {
          "alias": "__default"
        }
      },
      "auth/oauth2/{endpoint}": {
        "__path": {
          "alias": "oauth"
        }
      },
      "api/{endpoint}": {
        "__path": {
          "alias": "api"
        }
      }
    }
  }
}
